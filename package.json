{
  "name": "@vex_dev/sdk",
  "version": "0.1.1",
  "description": "The reliability layer for AI agents in production",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "author": "Oppla.ai <eng@oppla.ai>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Agent-X-AI/TypeScript-SDK"
  },
  "keywords": [
    "ai",
    "agents",
    "reliability",
    "verification",
    "vex"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
